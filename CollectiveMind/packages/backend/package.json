{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "seed:demo": "tsx src/scripts/seedDemoData.ts",
    "demo:verify": "node ../../verify-demo-setup.js",
    "demo:start": "npm run db:generate && npm run db:push && npm run seed:demo && npm run dev"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "winston": "^3.10.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "@prisma/client": "^5.5.0",
    "@elastic/elasticsearch": "^8.10.0",
    "@google-cloud/vertexai": "^1.4.0",
    "@google-cloud/aiplatform": "^3.5.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^16.3.0",
    "zod": "^3.22.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/uuid": "^9.0.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.1.0",
    "prisma": "^5.5.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
